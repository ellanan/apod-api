{
  "functions": {
    "api/index.ts": {
      "includeFiles": "prisma/**"
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "access-control-allow-origin",
          "value": "*"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "https://github.com/ellanan/apod-api"
    }
  ]
}
